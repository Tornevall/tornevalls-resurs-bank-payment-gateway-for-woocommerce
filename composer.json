{
    "name": "tornevall/resurs-bank-payment-gateway-for-woocommerce",
    "description": "Resurs Bank gateway for WooCommerce (third party release).",
    "minimum-stability": "stable",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Tomas Tornevall",
            "email": "thorne@tornevall.net"
        }
    ],
    "require": {
        "resursbank/ecom": "dev-master",
        "php": "8.1.*",
        "ext-curl": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "conflict": {},
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:resursbankplugins/ecom2.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "ResursBank\\": "src/",
            "Resursbank\\Woocommerce\\": "src/"
        }
    }
}
